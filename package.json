{
  "name": "express-typescript-starter",
  "version": "1.0.0",
  "description": "A robust Express.js starter kit with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/index.js",
    "build": "tsc && tsc-alias",
    "dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "express",
    "typescript",
    "nodejs",
    "starter",
    "ultimate-express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^5.0.1",
    "ultimate-express": "^2.0.9",
    "winston": "^3.17.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.9",
    "@types/node": "^24.0.12",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
